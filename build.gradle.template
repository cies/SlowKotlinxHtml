plugins {
    id 'org.jetbrains.kotlin.multiplatform' version 'KOTLIN_VERSION'
}
repositories {
    mavenCentral()
    flatDir {
        dirs 'libs'
    }
}
group 'com.example'
version '0.0.2'

apply plugin: 'maven-publish'

kotlin {
    jvm()
    sourceSets {
        jvmMain {
            dependencies {
                implementation("org.jetbrains.kotlinx:kotlinx-html:KOTLINXHTML_VERSION")
            }
        }
    }
}
